<Window x:Class="TPV.Views.wndFactura"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TPV.Views"
        mc:Ignorable="d"
        Title="Facturacio" Height="450" Width="800"
        DataContext="{Binding FacturaViewModel}"
        >
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <TextBlock Text="{Binding IdTicketString}" FontSize="20"/>
        <TextBlock Text="{Binding NumDocumentString}" FontSize="20"/>

    </StackPanel>
</Window>
